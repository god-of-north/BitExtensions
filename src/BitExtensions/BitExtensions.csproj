<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;net35</TargetFrameworks>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>GON.BitExtensions</Title>
    <FileVersion>1.1.0.0</FileVersion>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/god-of-north/BitExtensions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>bit setbit togglebit changebit</PackageTags>
    <PackageProjectUrl>https://github.com/god-of-north/BitExtensions</PackageProjectUrl>
    <Description>.NET Extensions to simplify work with bits</Description>
    <PackageId>GON.BitExtensions</PackageId>
    <Version>1.1.0</Version>
    <Authors>Maksym Lesyk</Authors>
    <Product>GON.BitExtensions</Product>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
